	<insert id="InsertByTheater" parameterType="theaterVo">
			<![CDATA[
				INSERT INTO theater 
				(theaterno, 
				 brandno, 
				 theatername, 
				 theateraddress, 
				 theaterxgps, 
				 theaterygps
				 )VALUES
				  (
				  #{theaterno},
				  #{brandno},
				  #{theatername},
				  #{theateraddress},
				  #{theaterxgps},
				  #{theaterygps}
				  )
				  ]]>
	</insert>
	
	<select id="SelectByTheater" resultType="theaterVo">
		<![CDATA[
			SELECT *
			FROM theater
		]]>
	</select>
	
	<select id="SelectByOneTheater" parameterType="String" resultType="theaterVo">
		<![CDATA[
			SELECT *
			FROM theater
			WHERE theatername = #{theatername}
		]]>
	</select>
</mapper>