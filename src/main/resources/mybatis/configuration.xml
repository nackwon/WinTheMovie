<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <!--경로 ex) com.javaex.vo.emaillistVo-->
        <!--<typeAlias alias="바꿀 이름" type="경로" />-->
        <typeAlias alias="theaterVo" type="kr.co.winthemovie.vo.TheaterVo"/>
        <typeAlias alias="nowpVo" type="kr.co.winthemovie.vo.NowpVo"/>
        <typeAlias alias="quickreserveVo" type="kr.co.winthemovie.vo.QuickReserveVo"/>
        <typeAlias alias="seatVo" type="kr.co.winthemovie.vo.SeatVo"/>
        <typeAlias alias="ticketVo" type="kr.co.winthemovie.vo.TicketVo"/>
    </typeAliases>
    <mappers>
        <!--xml 파일이 추가될 경우 아래에 계속 mapper 시켜줘야한다.-->
        <mapper resource="mybatis/mappers/users.xml"/>
        <mapper resource="mybatis/mappers/movie.xml"/>
        <mapper resource="mybatis/mappers/Theater.xml"/>
        <mapper resource="mybatis/mappers/tickets.xml"/>

    </mappers>
</configuration>